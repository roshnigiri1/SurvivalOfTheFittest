<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="model.PopulationGrowthOptionsController">
   <children>
      <MenuBar AnchorPane.bottomAnchor="670.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="Home">
               <items>
                  <MenuItem fx:id="home" mnemonicParsing="false" onAction="#homebutton" text="Home" />
               </items></Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem fx:id="help" mnemonicParsing="false" onAction="#handleButtonAction" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="deer" layoutX="368.0" layoutY="228.0" mnemonicParsing="false" onAction="#deerbutton" prefHeight="30.0" prefWidth="164.0" text="Deer" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="cattle" layoutX="368.0" layoutY="308.0" mnemonicParsing="false" onAction="#cattlebuton" prefHeight="30.0" prefWidth="164.0" text="Cattle" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="horses" layoutX="368.0" layoutY="383.0" mnemonicParsing="false" onAction="#horsesbutton" prefHeight="30.0" prefWidth="164.0" text="Horses" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Text layoutX="283.0" layoutY="137.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pick an animal to calculate pop. growth" textAlignment="CENTER" underline="true">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Text>
      <Text fill="#041208e6" layoutX="765.0" layoutY="672.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Survival of the Fittest" textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
